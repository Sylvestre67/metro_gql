type User {
  id: ID! @id
  name: String!
}

type Sample {
  id: ID! @id
  sample_tissue: String!
  z_score: Int!
  gene: String!
}

type Gene {
  id: ID! @id
  gene: String!
}

type SampleTissue{
  id: ID! @id
  tissue: String!
}

type Variant{
  id: ID! @id
  rsid: String
  type: String
  samples: Int
  chromosome: Int
  position: Int
  reference: String
  alternative: String
  effect: String
  impact: String
  feature_type: String
  feature_id: String
  hgvs_codon_change: String
  hgvs_protein_change: String
  clinvar_significance: String
  clinvar_disease: String
}
